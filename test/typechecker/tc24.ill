module TobyBreaksTypeChecking
  data List a = Cons a (List a) | Nil

  make_range :: Int -> List Int -> List Int
  fn make_range(x, acc)
    if x > 0 then
      make_range(x - 1, Cons(x, acc)) # issue is the inline constructor
    else
      acc
    end
  end

  range :: Int -> List Int
  fn range(x)
    make_range(x, Nil)
  end
end
